<nodes>
	<node name="plus" stylesheet="plus.xsl">
		<type> expression </type>
		<inputs>
			<slot sort="vec3">
				<name>in1</name>
				<source/>
				<value>1</value>
			</slot>


			<slot sort="vec3">
				<name>in2</name>
				<source/>
				<value>2</value>
			</slot>
		</inputs>
		<outputs>
			<slot type ="output" sort="vec3">
				<name>out1</name>
			</slot>
		</outputs>
	</node>	
	<node name="multiply" stylesheet="multiply.xsl">
		<type> expression </type>
		<inputs>
			<slot sort="int">
				<name>in1</name>
				<source/>
				<value>1</value>
			</slot>
			<slot sort="int">
				<name>in2</name>
				<source/>
				<value>2</value>
			</slot>
		</inputs>
		<outputs>
			<slot type ="output" sort="int">
				<name>out1</name>
			</slot>
		</outputs>
	</node>

</nodes>
