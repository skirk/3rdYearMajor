<?xml version="1.0" encoding="ISO-8859-1"?>
<root>
  <shader name="root" id="0" type="graph">
    <import name="vert_position" var="vec4"/>
    <import name="vert_normal" var="vec4"/>
    <export name="gl_Position" var="vec4">
      <source node="toEyeCoord" id="0" slot="eyepos"/>
    </export>
    <export name="outColor" var="vec4">
      <source node="toEyeCoord" id="0" slot="eyenormal"/>
    </export>
    <uniforms name="out1" type="mat4" parent="ModelView" id="0"/>
    <uniforms name="out1" type="mat4" parent="NormalMatrix" id="0"/>
    <node name="toEyeCoord" id="0" type="graph">
      <input name="position" var="vec4">
        <source node="root" id="0" slot="vert_position"/>
      </input>
      <input name="normal" var="vec4">
        <source node="root" id="0" slot="vert_normal"/>
      </input>
      <output name="eyepos" var="vec4">
        <source node="MultiplyMat4Vec4" id="0" slot="out1"/>
      </output>
      <output name="eyenormal" var="vec4">
        <source node="Multiply" id="0" slot="out1"/>
      </output>
      <node name="ModelView" id="0" type="state">
        <output name="out1" var="mat4"/>
      </node>
      <node name="NormalMatrix" id="0" type="state">
        <output name="out1" var="mat4"/>
      </node>
      <node name="MultiplyMat4Vec4" id="0" type="operator">
        <input name="in1" var="vec4">
          <source node="toEyeCoord" id="0" slot="position"/>
        </input>
        <input name="in2" var="mat4">
          <source node="ModelView" id="0" slot="out1"/>
        </input>
        <output name="out1" var="vec4"/>
      </node>
      <node name="Multiply" id="0" type="operator">
        <input name="in1" var="vec4">
          <source node="toEyeCoord" id="0" slot="normal"/>
        </input>
        <input name="in2" var="mat4">
          <source node="NormalMatrix" id="0" slot="out1"/>
        </input>
        <output name="out1" var="vec4"/>
      </node>
    </node>
  </shader>
</root>
